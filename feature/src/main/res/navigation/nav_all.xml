<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_all"
    app:startDestination="@id/menu_all_home">

    <fragment
        android:id="@+id/menu_all_navi_main"
        android:name="com.dongguk.telepigeon.feature.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/menu_all_navi_calender"
        android:name="com.dongguk.telepigeon.feature.calender.CalenderFragment"
        android:label="fragment_calender"
        tools:layout="@layout/fragment_calender" />

    <fragment
        android:id="@+id/menu_all_navi_setting"
        android:name="com.dongguk.telepigeon.feature.setting.SettingFragment"
        android:label="fragment_setting"
        tools:layout="@layout/fragment_setting" />

    <fragment
        android:id="@+id/menu_all_home"
        android:name="com.dongguk.telepigeon.feature.home.home.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home">
        <action
            android:id="@+id/action_home_to_main"
            app:destination="@id/menu_all_navi_main" />
        <action
            android:id="@+id/action_home_to_home_setting"
            app:destination="@id/menu_all_home_setting" />
    </fragment>

    <fragment
        android:id="@+id/menu_all_home_setting"
        android:name="com.dongguk.telepigeon.feature.home.setting.HomeSettingFragment"
        android:label="fragment_home_setting"
        tools:layout="@layout/fragment_home_setting" />
</navigation>